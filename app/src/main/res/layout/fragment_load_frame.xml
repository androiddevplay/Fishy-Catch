<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".openFrames.LoadFrame">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/load_scr">
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/load_l0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.06"/>
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/load_l1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.5"/>
        <ImageView
            android:id="@+id/fishy_logo_l"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:src="@drawable/load_logo"
            app:layout_constraintTop_toTopOf="@+id/load_l0"
            app:layout_constraintBottom_toBottomOf="@+id/load_l1"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>